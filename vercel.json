{
  "version": 2,
  "name": "dimar-site",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/index",
      "dest": "/index.html"
    },
    {
      "src": "/home",
      "dest": "/index.html"
    },
    {
      "src": "/sobre-nos",
      "dest": "/pages/sobre-nos.html"
    },
    {
      "src": "/sobre",
      "dest": "/pages/sobre-nos.html"
    },
    {
      "src": "/produtos",
      "dest": "/pages/produtos.html"
    },
    {
      "src": "/catalogo",
      "dest": "/pages/produtos.html"
    },
    {
      "src": "/produto",
      "dest": "/pages/produto.html"
    },
    {
      "src": "/produto/:id",
      "dest": "/pages/produto.html"
    },
    {
      "src": "/carrinho",
      "dest": "/pages/carrinho.html"
    },
    {
      "src": "/cart",
      "dest": "/pages/carrinho.html"
    },
    {
      "src": "/busca",
      "dest": "/pages/busca.html"
    },
    {
      "src": "/search",
      "dest": "/pages/busca.html"
    },
    {
      "src": "/contato",
      "dest": "/pages/contato.html"
    },
    {
      "src": "/lojas",
      "dest": "/pages/lojas.html"
    },
    {
      "src": "/admin",
      "dest": "/dimaradmin/login.html"
    },
    {
      "src": "/dimaradmin",
      "dest": "/dimaradmin/login.html"
    },
    {
      "src": "/dimaradmin/",
      "dest": "/dimaradmin/login.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/pages/$1"
    },
    {
      "src": "/dimaradmin/(.*)",
      "dest": "/dimaradmin/$1"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}


